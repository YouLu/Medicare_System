    <AppLogic key="SampleAppLogic">
      <Class>AppLogic.SampleAppLogic</Class>
    </AppLogic>
    <AppLogic key="SelfBloodAppLogic">
      <Class>AppLogic.SelfBloodAppLogic</Class>
    </AppLogic>
</AppLogicConfig>